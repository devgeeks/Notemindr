    "react-a11y": "^0.2.6",
    "react-redux": "^0.8.2",
    "react-router": "^1.0.0-beta3",
    "redux": "^1.0.1",
    "redux-thunk": "^0.1.0",
    "timeout-transition-group": "^1.0.3",
    "tinyvents": "^0.3.0"
  },
  "devDependencies": {
    "autoprefixer-loader": "^2.0.0",
