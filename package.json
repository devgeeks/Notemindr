{
  "name": "Notemindr",
  "version": "0.0.1",
  "description": "Notes app with reminders",
  "main": "src/app.js",
  "scripts": {
    "setup": "npm install && npm run build-dev",
    "setup-cordova": "./node_modules/.bin/cordova platform add android",
    "setup-global": "npm install -g webpack uglify-js jest-cli",
    "build": "webpack",
    "build-dev": "webpack",
    "start": "npm run build-dev && open www/index.html",
    "android": "./node_modules/.bin/cordova run android",
    "ios": "./node_modules/.bin/cordova run ios",
    "test": "./node_modules/.bin/jest"
  },
  "devDependencies": {
    "autoprefixer-loader": "^1.2.0",
    "babel-core": "^5.3.3",
    "babel-jest": "^5.2.0",
    "babel-loader": "^5.0.0",
    "cordova": "^4.1.2",
    "css-loader": "^0.12.0",
    "envify": "~1.2.1",
    "es6-promise": "~1.0.0",
    "extract-text-webpack-plugin": "^0.8.0",
    "fastclick": "^1.0.3",
    "flux": "^2.0.1",
    "jest-cli": "^0.4.0",
    "less-loader": "^2.2.0",
    "node-libs-browser": "^0.5.0",
    "path": "^0.4.9",
    "react": "~0.12.0",
    "react-a11y": "0.0.6",
    "react-async": "^2.0.1",
    "react-router": "^0.11.6",
    "react-tools": "^0.12.1",
    "style-loader": "^0.12.2",
    "timeout-transition-group": "0.0.2",
    "tinyvents": "^0.3.0",
    "uglify-js": "^2.4.15",
    "webpack": "^1.9.5"
  },
  "dependencies": {},
  "jest": {
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "scriptPreprocessor": "<rootDir>/jest-script-preprocessor",
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/timeout-transition-group",
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-tools"
    ]
  }
}
